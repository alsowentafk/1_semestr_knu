@{
    ViewBag.Title = "Home Page";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@Resources.Resource.DAI</title>
</head>
<body>
    <div>
        <h3>@Resources.Resource.Dtp</h3>
        <table>
            <tr>
                <td><p>@Resources.Resource.Name</p></td>
                <td><p>@Resources.Resource.Type</p></td>
                <td><p>@Resources.Resource.Price</p></td>
                <td></td>
            </tr>
            @foreach (var b in ViewBag.Dtps)
            {
                <tr>
                    <td><p> @b.Name </p></td>
                    <td><p> @b.Type </p></td>
                    <td><p> @b.Price </p></td>
                    <td><p> <a href="/Shtraf/Shtraf/@b.Id"> @Resources.Resource.Oform</a></p></td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
